SELECT COUNT(*) FROM UPDATE_FECHAS_SERIES_CLIENTES;
SELECT COUNT(*) FROM FECHAS_SERIE_CLIENTE;

SELECT L.INVENTARIO_ID, I.NO_CONTROL, F.NO_CONTROL, F.FECHA_SERIE, L.FECHAULTSERVSERIE
, L.FECHAULTSERVCLIENTE, F.FECHA_CLIENTE FROM FECHAS_SERIE_CLIENTE F INNER JOIN INVENTARIOS I ON I.NO_CONTROL=F.NO_CONTROL_TRIM
INNER JOIN LIBRES_INVENTARIOS L ON L.INVENTARIO_ID=I.INVENTARIO_ID;